      })\r\n    //     })\r\n    //     //If user's answer is incorrect, -1\r\n    //     db.collection(\"AnswerBank/\"+user+\"/Answers\").where('correct','==',false).onSnapshot((snapshot)=>{\r\n    //         snapshot.forEach((doc)=>{\r\n    //             score=score-1;\r\n    //             setPoints(score);\r\n    //         })\r\n    //     })\r\n    // },[])\r\n\r\n    //Store the points in the database\r\n    db.collection(\"Users\").doc(user).get().then((doc)=>{\r\n        setPoints(doc.data().score);\r\n    }).then(()=>{\r\n        setLoading(false);\r\n    })\r\n    return (loading?\r\n        <ThemeProvider theme={theme}>\r\n            <LinearProgress />\r\n        </ThemeProvider>\r\n        :\r\n        user?<div className=\"d-flex justify-content-center landing-page\">\r\n          <div>\r\n            <h1 style={{'color':'#E63946', 'fontWeight':'bolder', 'textAlign':'center','fontSize':'3rem'}}>\r\n                INDEPENDENCE DAY QUIZ\r\n            </h1>\r\n            <div className=\"d-flex justify-content-center\">\r\n                <img alt=\"trophy-img\" width=\"150px\" height=\"150px\" src=\"trophy.png\" />\r\n            </div>\r\n            <div style={{'border':'2px solid rgba(69, 123, 157,0.5)', 'borderRadius':'8px','height':'60px', 'width':'200px', 'margin':'auto', 'padding':'12px', 'color':'#f1faee'}}>\r\n                <h4 style={{'textAlign':'center'}}>{points}</h4>\r\n            </div>\r\n            <h6 style={{'color':'#f1faee', 'padding':'20px 50px', 'textAlign':'center'}}>Thankyou for taking the Quiz.\r\n                <br />\r\n                <br />\r\n                Team Quizzers' Club MANIT will release the leaderboard soon.<br /><br />Stay tuned.\r\n            </h6> \r\n            <div className=\"d-flex justify-content-center\">\r\n                <a href=\"/\"><button style={{'borderRadius':'8px', 'width':'250px'}} onClick={()=>{\r\n                    sessionStorage.removeItem(\"user\");\r\n                }}>Go to Landing Page</button></a>\r\n            </div>\r\n        </div>\r\n        <Brand />\r\n   </div>\r\n   :\r\n   <Redirect to=\"/\" />\r\n   )\r\n}","D:\\Projects\\QCM-recruitments\\src\\firebase.js",[],"D:\\Projects\\QCM-recruitments\\src\\components\\Brand.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\components\\Event.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\components\\TestNotStarted.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\components\\Timer.jsx",["94"],"import React,{ useEffect, useState }  from 'react'\r\n\r\nfunction Timer(props) {\r\n    const calculateTimeLeft = () => {\r\n        const countdownDate= sessionStorage.getItem(\"submitTime\");\r\n        const difference = countdownDate - +new Date();\r\n        let timeLeft = {};\r\n    \r\n        if (difference > 0) {\r\n          timeLeft = {\r\n            days: Math.floor(difference / (1000 * 60 * 60 * 24)),\r\n            hours: Math.floor((difference / (1000 * 60 * 60)) % 24),\r\n            minutes: Math.floor((difference / 1000 / 60) % 60),\r\n            seconds: Math.floor((difference / 1000) % 60),\r\n          };\r\n        }\r\n    \r\n        return timeLeft;\r\n      };\r\n\r\n      const [timeLeft, setTimeLeft] = useState(calculateTimeLeft());\r\n    \r\n      useEffect(() => {\r\n        setTimeout(() => {\r\n          setTimeLeft(calculateTimeLeft());\r\n        }, 1000);\r\n      });\r\n    // const [timeLeft, setTimeLeft]=useState(1800);\r\n    // useEffect(()=>{\r\n    //     setTimeout(() => {\r\n    //       setTimeLeft(timeLeft-1);\r\n    //     }, 1000);\r\n    // })\r\n    // let minutes=Math.floor(timeLeft/60);\r\n    // let seconds=timeLeft %60;\r\n\r\n    useEffect(()=>{\r\n      if(!timeLeft.minutes && !timeLeft.seconds){\r\n        props.func();\r\n      }\r\n    }, [timeLeft.seconds])\r\n\r\n    return (\r\n        <div className=\"Timer d-flex justify-content-center\">\r\n        <div style={{'fontWeight':'bold'}}>\r\n          <p style={{'textAlign':'center', 'margin':'0', 'padding':'0', 'color':timeLeft.minutes?'#f1faee':'#E63946'}}>\r\n            {timeLeft.minutes?(timeLeft.minutes<=9?\"0\"+timeLeft.minutes:timeLeft.minutes):\"00\"}\r\n            : \r\n            {timeLeft.seconds?(timeLeft.seconds<=9?\"0\"+timeLeft.seconds:timeLeft.seconds):\"00\"}\r\n          </p>        \r\n        </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Timer;\r\n","D:\\Projects\\QCM-recruitments\\src\\components\\SocialMedia.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\components\\Registration.jsx",["95"],"import {React, useState, useEffect} from 'react'\r\nimport {useLocation } from 'react-router-dom';\r\nimport firebaseApp from '../firebase';\r\nimport Brand from './Brand'\r\nimport Event from './Event'\r\nimport RegClosed from './RegClosed'\r\nimport {cities, states} from '../cities'\r\n\r\nexport default function Landing(){\r\n    const [name, setName]=useState(\"\");\r\n    const [college, setCollege]=useState(\"\");\r\n    const [email,setEmail]=useState(\"\");         //email\r\n    const [tel,setTel]= useState(\"\");           //mobile\r\n    const [city,setCity]=useState(\"\");         //first name\r\n    const [state,setState]=useState(\"\");             //last name\r\n    const [pwd,setPwd]=useState(\"\");             //post\r\n    const [rpwd,setRpwd]=useState(\"\");     //scholar no.\r\n    const [message, setMessage] =useState();\r\n    const [reg, setreg] = useState(localStorage.getItem(\"registered\"));\r\n\r\n    const handleChange=(e)=>{\r\n        setName(e.target.value);\r\n    }\r\n\r\n    //Email Validation\r\n    function validateEmail(emailAdd) {\r\n        const re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n        return re.test(String(emailAdd).toLowerCase());\r\n    }\r\n\r\n    //register user \r\n    const register = (event)=>{\r\n        event.preventDefault();\r\n        firebaseApp.firestore().collection(\"Users\").where(\"email\", \"==\", email).get().then((snapshot)=>{\r\n            if(snapshot.size){\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>You have already registered</p>);\r\n                setTimeout(() => {\r\n                    setMessage(\"\");\r\n                }, 2000);\r\n                return snapshot.size;\r\n            }\r\n        }).then((size)=>{\r\n            let flag=true;\r\n            if(size){\r\n                flag=false;\r\n            }\r\n            if(pwd.length < 8){\r\n                flag=false;\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Password should be atleast 8 characters long</p>);\r\n                setTimeout(() => {\r\n                    setMessage(\"\");\r\n                }, 2000);\r\n                return;\r\n            }\r\n            if(pwd !== rpwd){\r\n                flag=false;\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Passwords do not match</p>);\r\n                setTimeout(() => {\r\n                    setMessage(\"\");\r\n                }, 2000);\r\n                return;\r\n            }\r\n            if(name===\"\" || college===\"\" || city===\"\" || email===\"\" || tel===\"\" || state===\"\" || pwd===\"\" || rpwd === \"\"){\r\n                flag=false;\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Fill in all the details first</p>);\r\n                setTimeout(() => {\r\n                       setMessage(\"\");\r\n                }, 2000);\r\n                return;\r\n            }\r\n            \r\n            if(validateEmail(email)===false){\r\n                flag=false;\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Invalid Email</p>);\r\n                setTimeout(() => {\r\n                       setMessage(\"\");\r\n                }, 2000);\r\n                return;\r\n            }\r\n            \r\n            if(tel<1000000000 || tel>999999999999){\r\n                flag=false;\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Invalid Mobile Number.</p>);\r\n                setTimeout(() => {\r\n                       setMessage(\"\");\r\n                }, 2000);\r\n                return;\r\n            }\r\n    \r\n            if(flag){\r\n                firebaseApp.firestore().collection(\"Users\").doc(email).set({\r\n                    name: name,\r\n                    college: college,\r\n                    email:email,\r\n                    phone:tel,\r\n                    state: state,\r\n                    city: city,\r\n                    password:pwd,\r\n                    testAttempted:false\r\n                }).then(()=>{\r\n                    localStorage.setItem(\"registered\", true);\r\n                    setreg(true);\r\n                })\r\n            }\r\n        })\r\n\r\n        \r\n    };\r\n\r\n    //Render Form based on time\r\n    const [render, setRender]=useState(false);\r\n    const bypass=useLocation().search ===\"?bypass\";\r\n    useEffect(()=>{\r\n        let d=new Date().getTime();\r\n        let regClose= new Date(2021, 7, 14, 23, 59, 0, 0).getTime();\r\n\r\n        if(d<regClose || bypass){\r\n            setRender(true);\r\n        }\r\n    });\r\n\r\n    return(\r\n    <div className='landing-page'>\r\n        <Event />\r\n        <div style={{'color':'#f1faee', 'padding':'2% 10% ', 'textAlign':'justify', 'textAlignLast':'center'}}>\r\n            <p style={{'letterSpacing':'0.5px'}}>Quizzers Club MANIT is all set to take you on an audacious journey. Let us look back in time and ponder \r\n            upon every drop of blood that was shed and every person that was martyred to build us a dynamic \r\n            nation. Here's to the greater triumphs and achievements that await us. Are we brave and wise enough to leverage this opportunity and accept the challenge that our insurgents \r\n            left us? Take this quiz and discover the immortal lives that were lived, the endless wars that were fought \r\n            and the birthrights that were salvaged to give us everything that we have today! </p>\r\n        </div>\r\n        {reg?\r\n        <RegClosed info=\"You have successfully registered\" />\r\n        :render?<div>\r\n        <h3>Hello {name} !</h3>\r\n        <div className=\"d-flex justify-content-center\">\r\n            <div>\r\n                <input value={name} onChange={handleChange} type=\"text\" placeholder=\"Name\" required />\r\n                <input value={college} onChange={event=>setCollege(event.target.value)} type=\"text\" placeholder=\"College\" required />\r\n                <input type=\"email\" value ={email} onChange={event=>setEmail(event.target.value)}  placeholder=\"Email Address\" required />\r\n                <input value={tel} onChange={event=>setTel(event.target.value)} type=\"tel\" placeholder=\"Mobile Number\" required />\r\n                <select value={state} onChange={event=>setState(event.target.value)} required>\r\n                    <option value=\"\">Select a state</option>\r\n                    {states.map(st=>{\r\n                        return <option value={st}>{st}</option>\r\n                    })}\r\n                </select>\r\n                <br />\r\n                <select value={city} onChange={event=>setCity(event.target.value)} required>\r\n                    <option value=\"\">Select a city</option>\r\n                    {cities.map(c=>{\r\n                        if(c.state === state){\r\n                            return <option value={c.name}>{c.name}</option>\r\n                        }\r\n                        return null;\r\n                    })}\r\n                </select>\r\n                <input value={pwd} onChange={event=>setPwd(event.target.value)} type=\"password\" placeholder=\"Create Password\" required />\r\n                <input value={rpwd} onChange={event=>setRpwd(event.target.value)} type=\"password\" placeholder=\"Re-enter Password\" required />\r\n            </div>\r\n        </div>\r\n        <div className=\"d-flex justify-content-center\"><button onClick={register}>Register</button></div>\r\n        {message}\r\n        <br />\r\n        </div>\r\n        :\r\n        <RegClosed info=\"Registrations Closed.\" />\r\n        }\r\n        <Brand />\r\n    </div>\r\n    )\r\n}","D:\\Projects\\QCM-recruitments\\src\\components\\RegClosed.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\components\\Loader.jsx",[],"D:\\Projects\\QCM-recruitments\\src\\cities.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":84,"column":5,"nodeType":"102","endLine":84,"endColumn":14,"suggestions":"103"},{"ruleId":"104","severity":1,"message":"105","line":22,"column":12,"nodeType":"102","messageId":"106","endLine":22,"endColumn":19},{"ruleId":"100","severity":1,"message":"107","line":106,"column":6,"nodeType":"108","endLine":106,"endColumn":15,"suggestions":"109"},{"ruleId":"110","severity":1,"message":"111","line":114,"column":26,"nodeType":"112","messageId":"113","endLine":114,"endColumn":28},{"ruleId":"104","severity":1,"message":"114","line":1,"column":26,"nodeType":"102","messageId":"106","endLine":1,"endColumn":35},{"ruleId":"100","severity":1,"message":"115","line":41,"column":8,"nodeType":"108","endLine":41,"endColumn":26,"suggestions":"116"},{"ruleId":"100","severity":1,"message":"101","line":113,"column":5,"nodeType":"102","endLine":113,"endColumn":14,"suggestions":"117"},"no-native-reassign",["118"],"no-negated-in-lhs",["119"],"react-hooks/exhaustive-deps","React Hook useEffect contains a call to 'setRender'. Without a list of dependencies, this can lead to an infinite chain of updates. To fix this, pass [bypass] as a second argument to the useEffect Hook.","Identifier"