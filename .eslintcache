          <br />\r\n            <span style={{'color':'#E63946', 'fontSize':'1.4rem'}}>\r\n                MANIT\r\n            </span>\r\n        </div>\r\n    </div>)\r\n}\r\n\r\nconst LoginPage=()=>{\r\n    const history = useHistory();\r\n    const [email, setEmail] = useState('');\r\n    const [password, setPassword] = useState('');\r\n    const [message, setMessage] =useState();\r\n    const handleClick=(e)=>{\r\n        e.preventDefault();\r\n        //Checking the admin credentials with the onses stored in the database\r\n        db.collection(\"admin-credentials\").onSnapshot((snapshot)=>{\r\n        snapshot.forEach((doc)=>{\r\n            if(doc.data().LoginId===email && doc.data().Password === password){\r\n                history.push(\"/admin\");\r\n            }else{\r\n                setMessage(<p style={{'color':'#E63946', 'textAlign':'center'}}>Wrong Credentials</p>);\r\n                setTimeout(()=>{\r\n                  setMessage(\"\");\r\n                },2000)\r\n            }\r\n        })\r\n    }) \r\n    }\r\n\r\n    return (<div className=\"landing-page\">\r\n        <h1 style={{'color':'#E63946', 'fontWeight':'bolder', 'textAlign':'center','fontSize':'3rem'}}>\r\n            V<span style={{'fontSize':'2.5rem'}}>I</span>H<span style={{'fontSize':'2.5rem'}}>AA</span>N\r\n        </h1>\r\n        <input value={email} onChange={(e)=>setEmail(e.target.value)} type=\"text\" placeholder=\"QCM Unique ID\" />\r\n        <input value={password} onChange={(e)=>setPassword(e.target.value)} type=\"password\" placeholder=\"Password\" />\r\n        <a>\r\n            <button onClick={handleClick}>\r\n                Login\r\n            </button>\r\n        </a>\r\n        {message}\r\n    </div>)\r\n}\r\n\r\n","E:\\Programming\\Learning Web Development\\heroku-test-qcm\\src\\components\\RegistrationPage.jsx",["87","88"],"E:\\Programming\\Learning Web Development\\heroku-test-qcm\\src\\components\\Score.jsx",["89","90","91"],"import React, {useState, useEffect} from 'react'\r\nimport {db} from '../firebase'\r\nimport {useHistory} from \"react-router-dom\"\r\n\r\nexport default function Score(){\r\n\r\n    //Score Calculation\r\n    let score=0;\r\n    const [points, setPoints] =useState(0);\r\n    useEffect(()=>{\r\n        //If user didn't answer no increment no decrement in points\r\n        db.collection(\"AnswerBank/\"+sessionStorage.getItem(\"name\")+\"-\"+sessionStorage.getItem(\"sch\")+\"/Answers\").where('userAns','==','').onSnapshot((snapshot)=>{\r\n            snapshot.forEach((doc)=>{\r\n                setPoints(score);\r\n            })\r\n        })\r\n        //If user's answer is correct, +4\r\n        db.collection(\"AnswerBank/\"+sessionStorage.getItem(\"name\")+\"-\"+sessionStorage.getItem(\"sch\")+\"/Answers\").where('correct','==',true).onSnapshot((snapshot)=>{\r\n            snapshot.forEach((doc)=>{\r\n                score=score+4;\r\n                setPoints(score);\r\n            })\r\n        })\r\n        //If user's answer is incorrect, -1\r\n        db.collection(\"AnswerBank/\"+sessionStorage.getItem(\"name\")+\"-\"+sessionStorage.getItem(\"sch\")+\"/Answers\").where('correct','==',false).onSnapshot((snapshot)=>{\r\n            snapshot.forEach((doc)=>{\r\n                score=score-1;\r\n                setPoints(score);\r\n            })\r\n        })\r\n    },[])\r\n\r\n    //Store the points in the database\r\n    db.collection(\"scores\").doc(sessionStorage.getItem(\"name\")+\"-\"+sessionStorage.getItem(\"sch\")).set({\r\n        points:points\r\n    });\r\n    \r\n    return (<div className=\"d-flex justify-content-center landing-page\">\r\n        <div>\r\n            <h1 style={{'color':'#E63946', 'fontWeight':'bolder', 'textAlign':'center','fontSize':'3rem'}}>\r\n                V<span style={{'fontSize':'2.5rem'}}>I</span>H<span style={{'fontSize':'2.5rem'}}>AA</span>N\r\n            </h1>\r\n            <div className=\"d-flex justify-content-center\">\r\n                <img alt=\"trophy-img\" width=\"150px\" height=\"150px\" src=\"trophy.png\" />\r\n            </div>\r\n            <h3></h3>\r\n            <div style={{'border':'2px solid rgba(69, 123, 157,0.5)', 'borderRadius':'8px','height':'60px', 'width':'200px', 'margin':'auto', 'padding':'12px', 'color':'#f1faee'}}>\r\n                <h4 style={{'textAlign':'center'}}>{points}</h4>\r\n            </div>\r\n            <h6 style={{'color':'#f1faee', 'padding':'20px 50px', 'textAlign':'center'}}        >Thankyou for taking the Test.\r\n                <br />\r\n                <br />\r\n                Team Quizzers' Club MANIT will release the dashboard soon.<br /><br />Stay tuned.\r\n            </h6>    \r\n        </div>\r\n        <div className=\"brand\">\r\n            Quizzers' Club\r\n            <br />\r\n            <span style={{'color':'#E63946', 'fontSize':'1.4rem'}}>MANIT</span>\r\n        </div>\r\n   </div>)\r\n}",{"ruleId":"92","severity":1,"message":"93","line":1,"column":16,"nodeType":"94","messageId":"95","endLine":1,"endColumn":24},{"ruleId":"92","severity":1,"message":"96","line":11,"column":17,"nodeType":"94","messageId":"95","endLine":11,"endColumn":26},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"97","replacedBy":"101"},{"ruleId":"99","replacedBy":"102"},{"ruleId":"92","severity":1,"message":"93","line":2,"column":9,"nodeType":"94","messageId":"95","endLine":2,"endColumn":17},{"ruleId":"92","severity":1,"message":"103","line":3,"column":49,"nodeType":"94","messageId":"95","endLine":3,"endColumn":57},{"ruleId":"104","severity":1,"message":"105","line":33,"column":5,"nodeType":"106","endLine":33,"endColumn":14,"suggestions":"107"},{"ruleId":"108","severity":1,"message":"109","line":55,"column":24,"nodeType":"110","messageId":"111","endLine":55,"endColumn":26},{"ruleId":"112","severity":1,"message":"113","line":48,"column":9,"nodeType":"114","endLine":48,"endColumn":12},{"ruleId":"92","severity":1,"message":"115